@model IEnumerable<XenaBudgetManager.Models.Fiscal>

@{
    ViewBag.Title = "Virksomheder";
}

<h2>Virksomheder</h2>

<table class="table table-hover">
    <tr>
        <th>
            <p>Virksomheder:</p>
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.ActionLink(item.FiscalSetupName, "SetFiscalAndUser", new { item.FiscalID, item.FiscalSetupName, item.UserID })
        </td>
    </tr>
}

</table>
