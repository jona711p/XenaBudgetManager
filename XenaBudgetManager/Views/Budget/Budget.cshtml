@using System.Data
@model DataSet
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Budget</title>
</head>
<body>
    <table cellpadding="0" cellspacing="0">
        <tr>
            <th>BudgetID</th>
            <th>BudgetName</th>
            <th>Year</th>
        </tr>
        @foreach (DataRow row in Model.Tables[0].Rows)
        {
            <tr>
                <td>@row["BudgetID"]</td>
                <td>@row["BudgetName"]</td>
                <td>@row["Year"]</td>
            </tr>
        }
    </table>
</body>
</html>